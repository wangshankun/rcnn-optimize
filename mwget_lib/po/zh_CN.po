msgid "Usage: "
msgstr "用法："

#: ../mwget-0.1.0/src/mwget.cpp:39
#: ../mwget-0.1.0/src/mwget.cpp:59
msgid " [Options]... [URL]..."
msgstr "[选项]....[URL]"

#: ../mwget-0.1.0/src/mwget.cpp:41
msgid "Options:"
msgstr "选项："

#: ../mwget-0.1.0/src/mwget.cpp:42
msgid "  -b,  --debug          Show the debug message"
msgstr "  -b,  --debug          调试模式，显示调试信息"

#: ../mwget-0.1.0/src/mwget.cpp:43
msgid "  -c,  --count=num      Set the retry count to [num], no limit when \"0\", the default is \"99\""
msgstr "  -c,  --count=num      设置重试次数为[num],不限制次数设置为“0“，默认设置为“99”。"

#: ../mwget-0.1.0/src/mwget.cpp:44
msgid "  -d,  --directory=dir  Set the local direcotry to [dir], the default is \".\""
msgstr "  -d,  --directory=dir  设置本地目录为[dir],默认值为当前目录。"

#: ../mwget-0.1.0/src/mwget.cpp:45
msgid "  -f,  --file=file      Rename the file to [file]"
msgstr "  -f,  --file=file      重命名下载后文件为[file]"

#: ../mwget-0.1.0/src/mwget.cpp:46
msgid "  -h,  --help           A brief summary of all the options"
msgstr "  -h,  --help          显示帮助信息。"

#: ../mwget-0.1.0/src/mwget.cpp:47
msgid "  -i,  --interval=num   Set the ftp retry interval to [num] seconds, the default is \"5\""
msgstr "  -i,  --interval=num    设置FTP重试期限为[num]秒，默认为“5“。"

#: ../mwget-0.1.0/src/mwget.cpp:48
msgid "  -n,  --number=num     Use [num] connections instead of the default (4)"
msgstr "  -n,  --number=num     设置下载的线程数，默认开4个线程。"

#: ../mwget-0.1.0/src/mwget.cpp:49
msgid "  -r,  --referer=URL    Include `Referer: [URL]' header in HTTP request."
msgstr "  -r,  --referer=URL    使用“Referer: [URL]”在HTTP头中欺骗服务器。"

#: ../mwget-0.1.0/src/mwget.cpp:50
msgid "  -t,  --timeout=num    Set the connection timeout to [num] seconds, the default is \"30\""
msgstr "  -t,  --timeout=num    设置超时时间为[num]秒，默认设置是“30”。"

#: ../mwget-0.1.0/src/mwget.cpp:51
msgid "  -v,  --version        Show the version of the mwget and exit"
msgstr "  -v,  --version        显示mwget的版本，然后退出。"

#: ../mwget-0.1.0/src/mwget.cpp:52
msgid "  -x,  --proxy=URL      Set the proxy [URL]"
msgstr "  -x,  --proxy=URL      设置代理 [URL]"

